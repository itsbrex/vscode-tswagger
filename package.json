{
  "name": "swagger-typescript-generator",
  "displayName": "swagger-typescript-generator",
  "description": "swagger-typescript-generator",
  "version": "0.0.1",
  "private": "true",
  "author": "Orchard",
  "publisher": "Orchard",
  "repository": "https://github.com/Orchardxyz/vscode-swagger-typescript-generator.git",
  "keywords": [
    "vscode",
    "vscode extension",
    "swagger",
    "typescript",
    "swagger-to-typescript"
  ],
  "workspaces": {
    "packages": [
      "webview"
    ],
    "nohoist": [
      "**"
    ]
  },
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "swagger-typescript-generator.generateTypescript",
        "title": "GenerateTypescriptForSwagger"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "SwaggerTypescriptGenerator",
      "properties": {
        "SwaggerTypescriptGenerator.remoteUrlList": {
          "type": "array",
          "description": "配置 swagger 接口列表"
        }
      }
    }
  },
  "scripts": {
    "webview-build": "pnpm --filter stg-webview build",
    "webview-watch": "pnpm --filter stg-webview start",
    "vscode:prepublish": "npm run webview-build && npm run esbuild-build -- --minify",
    "esbuild-build": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node",
    "esbuild-watch": "esbuild ./src/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --sourcemap --watch --define:process.env.NODE_ENV='development'",
    "test-compile": "tsc -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/lodash-es": "^4.17.7",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.77.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "chokidar": "^3.5.3",
    "esbuild": "^0.17.15",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "openapi-types": "^12.1.0",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "bing-translate-api": "^2.9.1",
    "directory-tree": "^3.5.1",
    "json-schema-to-typescript": "^12.0.0",
    "lodash-es": "^4.17.21"
  }
}
